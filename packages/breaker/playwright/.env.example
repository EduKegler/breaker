# URL do grafico no TradingView
TV_CHART_URL="https://br.tradingview.com/chart/ovLwxUsu/"

# Caminho para strategy local (.pine). Exemplo: ../../v4_btc_strategy_daily_stop_usd.pine
STRATEGY_FILE="../../v4_btc_strategy_daily_stop_usd.pine"

# Estado salvo de autenticacao (gerado pelo script de login)
AUTH_FILE=".auth/tradingview.json"

# Nome para salvar o script novo no TradingView (opcional)
# Se vazio, usa o nome do strategy(...) + " - AUTO"
SAVE_SCRIPT_NAME=""

# true para rodar sem interface grafica
HEADLESS="false"

# Timeout geral em ms
TIMEOUT_MS="30000"

# true para abrir o navegador em fullscreen
FULLSCREEN="true"

# Logs detalhados por etapa
VERBOSE_LOGS="true"

