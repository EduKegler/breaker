// @version=6
// Minimal SMA crossover for E2E smoke test â€” compiles fast, generates trades
strategy("E2E Smoke Test", overlay=true, default_qty_type=strategy.fixed, default_qty_value=1)

fastLen = 10
slowLen = 30

fast = ta.sma(close, fastLen)
slow = ta.sma(close, slowLen)

if ta.crossover(fast, slow)
    strategy.entry("Long", strategy.long)

if ta.crossunder(fast, slow)
    strategy.close("Long")

plot(fast, "Fast SMA", color.blue)
plot(slow, "Slow SMA", color.red)
